public String removeBracketsAndComma(String field)
{
	if(field!=null)
	{
		if(field.contains("["))
		{
			field=field.replace("[","")
			field=field.replace("]","")
		}
		if (field.contains(","))
		{
			field=field.replace(","," ")
		}
	}
	return field
}

