public String removeBracketAndComma(String field)
{
	if(field!=null)
	{
		if(field.contains("["))
		{
			field=field.replace("[","")
			field=field.replace("]","")
		}
		if(field.contains(","))
		{
			field=field.replace(","," ")
		}
		if(field.contains(" "))
		{
			field = field.replaceAll("\\s","")	
		}
	}
	return field
}

