from ApplicationBase AB
inner join ApplicationMemberAssured AMA on AMA.Application_Id =AB.Id
inner join ApplicationRisk AR on AR.ApplicationMemberAssured_Id = AMA.Id
inner join Risk R on R.Id=AR.Risk_Id

