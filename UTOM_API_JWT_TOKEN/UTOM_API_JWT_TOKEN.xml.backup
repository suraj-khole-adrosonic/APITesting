public String removeComma(String field)
{
	if(field!=null && field.contains(","))
		field=field.replace(","," ")
	if(field!=null && field.contains("["))
	{
		field=field.replace("[","")
		field=field.replace("]","")
	}
	if(field.contains(".00"))
	{
		field = field.replace(".00","")
	}
	return field
}
